<!DOCTYPE html>
<html lang="en">
  <!-- Head Section -->
  <%- include('partials/head', { title: 'Coupon Site'}) %>

  <body>
    <!-- Preloader -->
    <%- include('partials/preloader') %>

    <div class="contact section" id="pick-location">
      <div class="container">
        <div class="row">
          <div class="col-lg-4 offset-lg-4">
            <div class="section-heading text-center">
              <h6>| Details Form</h6>
              <h2>Select Property Location & fill in your Details</h2>
            </div>
          </div>
        </div>
      </div>
    </div>

    <div class="contact-content">
      <div class="container">
        <div class="row">
          <div class="col-lg-7">
            <div id="map"></div>
            <div class="row text-center">
              <h6 id="confirm-location" class="text-primary">
                Pick a Location
              </h6>
            </div>
          </div>
          <div class="col-lg-5">
            <form
              class="contact-form"
              id="details-form"
              action=""
              method="post"
            >
              <div class="row">
                <div class="col-lg-12">
                  <fieldset>
                    <label for="name">Full Name</label>
                    <input
                      type="name"
                      name="name"
                      id="name"
                      placeholder="Your Name..."
                      autocomplete="on"
                      required
                    />
                  </fieldset>
                </div>
                <div class="col-lg-12">
                  <fieldset>
                    <label for="email">Email Address</label>
                    <input
                      type="text"
                      name="email"
                      id="email"
                      pattern="[^ @]*@[^ @]*"
                      placeholder="Your E-mail..."
                      required
                    />
                  </fieldset>
                </div>
                <div class="col-lg-12">
                  <fieldset>
                    <label for="phone">Phone Number</label>
                    <input
                      type="tel"
                      name="phone"
                      id="phone"
                      placeholder="Your Number..."
                      required
                    />
                  </fieldset>
                </div>
                <div class="col-lg-12">
                  <fieldset>
                    <button type="submit" id="form-submit" class="btn-danger">
                      Submit Details
                    </button>
                  </fieldset>
                </div>
              </div>
            </form>
          </div>
        </div>
      </div>
    </div>

